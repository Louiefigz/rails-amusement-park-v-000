<h1>Attractions#show</h1>
<p>Find me in app/views/attractions/show.html.erb</p>






<h1>Go on <%= @attraction.name %></h1>


<%= form_tag rides_path %>
<%= tag(:input, :type => "hidden", :name => request_forgery_protection_token.to_s, :value => form_authenticity_token) %>
<%= tag(:input, :type => "hidden", :name => :user_id, :value => session[:user_id]) %>
<%= tag(:input, :type => "hidden", :name => :attraction_id, :value => @attraction.id) %>
<input type="submit" value="Go on this ride">


<%= link_to "Edit Attraction", edit_attraction_path(@attraction) %>
